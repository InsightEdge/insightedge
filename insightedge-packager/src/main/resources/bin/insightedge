#!/bin/bash
DIRNAME=$(dirname ${BASH_SOURCE[0]})
source ${DIRNAME}/../insightedge/conf/insightedge-env.sh
GS_COMMAND="`$JAVACMD -cp "$XAP_HOME/lib/required/*:$XAP_HOME/tools/cli/*" org.insightedge.cli.commands.I9ECommandFactory cli-insightedge` $*"
if [[ $GS_COMMAND == Error:* ]]; then
    echo $GS_COMMAND
else
    if [[ $GS_VERBOSE == "true" ]]; then
        echo Executing GigaSpaces command:
        echo $GS_COMMAND
        echo --------------------------------------------------------------------------------
    fi
    $GS_COMMAND
fi
